@model IEnumerable<GestionCV.Models.ExperienceProfessionnelle>



@foreach (var i in Model)
{
    <li class="collection-item row">
        <div class="valign-wrapper col s10">
            @Html.DisplayFor(modelItem => i.TitrePoste) en @Html.DisplayFor(modelItem => i.Entreprise)
            @Html.DisplayFor(modelItem => i.AnneeDebut), @Html.DisplayFor(modelItem => i.AnneeFin)
        </div>

        <div class="col s2 right">
            <a asp-controller="ExperiencesProfessionnelles" asp-action="Edit" asp-route-id="@i.ExperienceProfessionnelleId" class="btn-floating btn-small blue darken-4"><i class="material-icons">edit</i></a>
            <a onclick="AfficherModal(@i.ExperienceProfessionnelleId, 'ExperiencesProfessionnelles')" class="btn-floating btn-small red darken-4"><i class="material-icons">delete_forever</i></a>
        </div>
    </li>

}