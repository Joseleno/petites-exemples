@model IEnumerable<GestionCV.Models.Objectif>

@foreach (var i in Model)
{
    <li class="collection-item row">
        <div class="valign-wrapper col s10">
            @Html.DisplayFor(modelItem => i.Description)
        </div>

        <div class="col s2 right">
            <a asp-controller="Objectifs" asp-action="Edit" asp-route-id="@i.ObjectifId" class="btn-floating btn-small blue darken-4"><i class="material-icons">edit</i></a>
            <a onclick="AfficherModal(@i.ObjectifId, 'Objectifs')" class="btn-floating btn-small red darken-4"><i class="material-icons">delete_forever</i></a>
        </div>
    </li>
}

