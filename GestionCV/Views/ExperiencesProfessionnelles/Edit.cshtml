@model GestionCV.Models.ExperienceProfessionnelle

@{
    ViewData["Title"] = "Misse à jour experience profissionelle";
}

<div class="formulaire">
    <h4>Misse à jour experience profissionelle</h4>
    <hr />
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Edit">
                <input type="hidden" asp-for="ExperienceProfessionnelleId" />
                <partial name="_DonneesFormulaire" />
                <div class="row">
                    <input type="submit" value="Confirmer" class="btn btn-small orange darken-2" />
                    <a asp-action="Details" asp-controller="Curriculums" asp-route-id="@Model.CurriculumId" class="btn btn-small orange lighten-1">Retour</a>
                </div>
            </form>
        </div>
    </div>
</div>