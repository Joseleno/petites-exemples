@model IEnumerable<GestionCV.Models.Langue>

@foreach (var i in Model)
{
    <li class="collection-item row">
        <div class="valign-wrapper col s10">
            @Html.DisplayFor(modelItem => i.Nom)
            @Html.DisplayFor(modelItem => i.Niveau)
        </div>

        <div class="col s2 right">
            <a asp-controller="Langues" asp-action="Edit" asp-route-id="@i.LangueId" class="btn-floating btn-small blue darken-4"><i class="material-icons">edit</i></a>
            <a onclick="AfficherModal(@i.LangueId, 'Langues')" class="btn-floating btn-small red darken-4"><i class="material-icons">delete_forever</i></a>
        </div>
    </li>
}

